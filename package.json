{
  "name": "wblot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "update": "npm-check-updates --upgrade && npm update",
    "format": "prettier --write .",
    "check": "tsc --noEmit --skipLibCheck",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "konva": "^8.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-konva": "^18.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "gh-pages": "^5.0.0",
    "npm-check-updates": "^16.7.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.2"
  },
  "prettier": {
    "printWidth": 9999999
  }
}
